{
  "name": "web-apollo",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:generate-output": "jest --json --outputFile=jest-test-results.json",
    "dev": "npm run generate-imported-component",
    "dev:client": "npm run generate-imported-component && parcel src/app/index.html",
    "dev:server": "parcel src/server/index.ts",
    "build": "rimraf dist && npm run generate-imported-component && npm run build-client && npm run build-server",
    "build-client": "cross-env BABEL_ENV=client parcel build src/app/index.html -d dist/client --public-url /dist",
    "build-server": "cross-env BABEL_ENV=server parcel build src/server/index.ts -d dist/server --public-url /dist --target=node10",
    "generate-imported-component": "imported-components src/app src/app/imported.ts",
    "start": "node dist/server",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
